<%- include('header'); -%>


<body>

<!-- Barra Lateral -->

<%- include('menuLateral'); -%>

<div class="container mt-5">
    <h3>Cadastro de Horários</h3>
    <form id="schedule-form">
        <div class="form-group">
            <label for="day">Dia da Semana:</label>
            <select id="day" name="day" class="form-control">
                <option value="segunda">Segunda-feira</option>
                <option value="terca">Terça-feira</option>
                <option value="terca">Quarta-feira</option>
                <option value="terca">Quinta-feira</option>
                <option value="terca">Sexta-feira</option>
                <option value="terca">Sábado</option>

            </select>
        </div>

        <div class="form-group">
            <label for="time">Horário:</label>
            <input type="time" id="time" name="time" required class="form-control">
        </div>


        <button type="submit" class="btn btn-primary">Salvar</button>
        <button type="button" class="btn btn-secondary">Cancelar</button>
    </form>
</div>

</body>



<%- include('footer'); -%>